{
  "name": "@anukit/core",
  "version": "0.1.0",
  "type": "module",
  "description": "Core utilities and providers for AnuKit component library",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./providers/SSRProvider": {
      "types": "./dist/providers/SSRProvider.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./hooks/accessibility-hooks": {
      "types": "./dist/hooks/accessibility-hooks.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./hooks/ssr-hooks": {
      "types": "./dist/hooks/ssr-hooks.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./utils/ssr-utils": {
      "types": "./dist/utils/ssr-utils.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.2.0",
    "rollup-plugin-swc3": "^0.10.3",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/optimui/optimui.git",
    "directory": "packages/core"
  },
  "keywords": [
    "react",
    "components",
    "ssr",
    "performance",
    "typescript",
    "accessibility"
  ],
  "license": "MIT"
}